if ("plotrix" %in% rownames(installed.packages()) == FALSE){
install.packages("plotrix")
}
library(plotrix)
# Create data for the graph.
x <-  c(56,35,9)
lbl <-  c("56%","35%","9%")
# Give the chart file a name.
#png(file = "SmartphonesOwnership_pie_chart.jpg")
# Plot the chart.
pie3D(x,labels = lbl,explode = 0.1, main = "Smartphone Ownership in U.S.(2013) ", col=c("#7570b3","#66a61e","#d95f02"), radius = 0.9)
pie3D
